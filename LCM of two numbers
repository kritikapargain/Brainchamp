#include<stdio.h>
int main ()
{
int num1,num2,max_value;
printf("Please enter two integer values\n");
scanf("%d %d",&num1,&num2);
max_value=(num1>num2)? num1:num2);
while(1)
{
if(max_value %num1==0 &&max_value %num2==0)
{
printf("LCM of %d and %d=%d",num1,num2,max_value);
break;
}
++max_value;
} 
return 0;
}



